      <div class="tile is-ancestor" style="overflow: hidden; overflow-x: scroll;">
        <% for (var i = 0; i < response.length; i++) { %>
          <% if (response[i].pouch_price) { %>
            <div class="tile is-parent">
              <article class="tile is-child box" style="width:329px;">
                <p class="title" style="font-family: 'Montserrat', sans-serif"><%= response[i].pouch_name %></p>
                <figure class="image is-4by3">
                  <img src="<%= response[i].pouch_image_url_2 %>" alt="CaloBox ~ <%= response[i].pouch_name %>">
                </figure>
                <br />
                <p class="subtitle is-6"><%= response[i].quick_desc %></p>
                <b>Rs. <%= response[i].pouch_price %> (<%= response[i].net_weight %> gms)</b>
                <button
                  class="snipcart-add-item button is-medium is-primary"
                  data-item-id="<%= response[i].pouch_id %>"
                  data-item-name="<%= response[i].pouch_name %>"
                  data-item-price="<%= response[i].pouch_price %>"
                  data-item-weight="<%= response[i].net_weight %>"
                  data-item-url="/individual-snacks/<%= response[i].pouch_name %>"
                  data-item-image="<%= response[i].pouch_image_url %>"
                  data-item-has-taxes-included="false">
                      Add to Cart
                </button>
              </article>
            </div>
          <% } %>
        <% } %>
      </div>
    </section>

